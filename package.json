{
  "name": "rsa-key-pair",
  "version": "1.0.1-0",
  "description": "A library for generating and working with RSA key pairs, for use with crypto-ld.",
  "homepage": "https://github.com/digitalbazaar/rsa-key-pair",
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/rsa-key-pair"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/rsa-key-pair/issues",
    "email": "support@digitalbazaar.com"
  },
  "license": "BSD-3-Clause",
  "main": "src/RSAKeyPair.js",
  "files": [
    "src/*.js"
  ],
  "dependencies": {
    "base64url-universal": "^1.1.0",
    "bs58": "^4.0.1",
    "crypto-ld": "digitalbazaar/crypto-ld#extract-key-pairs",
    "node-forge": "~0.9.1",
    "semver": "^7.3.2"
  },
  "optionalDependencies": {
    "sodium-native": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "babel-loader": "^8.1.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-digitalbazaar": "^2.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "karma": "^5.0.9",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tap-reporter": "0.0.6",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "multibase": "^0.7.0",
    "multicodec": "^1.0.1",
    "multihashes": "^0.4.19",
    "nyc": "^15.1.0",
    "webpack": "^4.43.0"
  },
  "nyc": {
    "exclude": [
      "tests"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "browser": {
    "bs58": false,
    "crypto": false,
    "sodium-native": false,
    "util": false,
    "semver": false
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "Decentralized",
    "DID",
    "Credential",
    "Cryptography",
    "Linked Data"
  ],
  "scripts": {
    "test": "npm run test-node",
    "test-node": "cross-env NODE_ENV=test mocha --preserve-symlinks -t 30000 -A -R ${REPORTER:-spec} tests/*.spec.js",
    "test-karma": "karma start",
    "benchmark": "node benchmark/benchmark.js",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm run test-node",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=text-lcov npm run test-node > coverage.lcov",
    "coverage-report": "nyc report",
    "lint": "eslint src tests"
  }
}
